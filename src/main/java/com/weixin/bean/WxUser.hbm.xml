<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.weixin.bean.WxUser" table="wx_user" schema="" catalog="wx_admin">
        <id name="id" column="id"/>
        <property name="name" column="name"/>
        <property name="departmentId" column="department_id"/>
        <property name="positionId" column="position_id"/>
        <property name="mobile" column="mobile"/>
        <property name="gender" column="gender"/>
        <property name="tel" column="tel"/>
        <property name="email" column="email"/>
        <property name="weixinId" column="weixin_id"/>
        <property name="enable" column="enable"/>
    </class>
</hibernate-mapping>